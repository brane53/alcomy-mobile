{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8101",
      "webRoot": "${workspaceRoot}/www",
      "sourceMaps": true
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 8101,
      "url": "http://localhost",
      "webRoot": "${workspaceRoot}/www",
      "sourceMaps": true
    }
  ]
}