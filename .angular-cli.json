{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ionic2-tdd"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets"
      ],
      "index": "index.html",
      "main": "app/main.ts",
      "polyfills": "",
      "test": "test.ts",
      "tsconfig": "",
      "testTsconfig": "",
      "prefix": "",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "src/environments/environment.ts",
      "environments": {
        "dev": "src/environments/environment.ts",
        "prod": "src/environments/environment.prod.ts"
      }
    }
  ],
  "lint": [],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "class": {
      "spec": false
    },
    "component": {
      "inlineStyle": false,
      "inlineTemplate": false,
      "spec": true
    },
    "directive": {
      "spec": true
    },
    "module": {
      "spec": false
    },
    "pipe": {
      "spec": true
    },
    "service": {
      "spec": true
    },
    "interface": {
      "prefix": "I"
    }
  }
}